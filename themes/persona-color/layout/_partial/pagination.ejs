<nav id="pagination" class="titlelist">
  <% if (page.prev){ %>
    <a href="<%- config.root %><%- page.prev_link %>" class="alignleft prev titlelist"><%= __('prev') %></a>
  <% } %>
  <% if (page.next){ %>
    <a href="<%- config.root %><%- page.next_link %>" class="alignright next titlelist"><%= __('next') %></a>
  <% } %>
  <div class="clearfix"></div>
</nav>
